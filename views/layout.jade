doctype html
html(lang='en')
	head
		title= title
		link(rel='stylesheet', href='/css/bootstrap_flatly.css')
		link(rel='stylesheet', href='/css/plzsq.css')
	style

	body
		nav.navbar.navbar-default
			div.container-fluid
				div.navbar-header
					a.navbar-brand
						img(src='/img/plazasq.svg', alt='Plaza Square')
						span= title
				div.collapse.navbar-collapse
					ul.nav.navbar-nav.navbar-right
						li
							a(href="#") Link
		div.container-fluid
			block content
		if useCdn
			script(type='text/javascript' src='//cdnjs.cloudflare.com/ajax/libs/jquery/1.10.2/jquery.min.js')
		else
			script(type='text/javascript' src='./js/jquery-1.10.2.min.js')
			script(type='text/javascript' src='./js/angular.1.5.7min.js')
			script(type='text/javascript' src='./js/bootstrap.3.3.7.min.js')
			script(type='text/javascript' src='./js/bootswatch-custom.js')
			script(type='text/javascript' src='./js/stomp.2.3.3.min.js')
			script(type='text/javascript' src='./js/sockjs.1.1.1.min.js')
			script(type='text/javascript' src='./js/canvasjs.1.7.0.min.js')
			script(type='text/javascript' src='./js/plazasq.js')
			script(type='text/javascript' src='./app/app.js')
			script(type='text/javascript' src='./app/controllers.js')
			script(type='text/javascript' src='./app/services.js')
