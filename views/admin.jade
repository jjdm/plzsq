extends layout

block content
	div.row
		div.col-lg-3(ng-controller='chartController')
			h4 Chart
			#tradingChart
			h4 History Log
			p More text here
		div.col-lg-9
			form.form-horizontal(ref='uploadForm', id='uploadForm', action='./config', method='post', encType='multipart/form-data')
				.form-group
					h4.col-lg-3 Trader Summary
					label.control-label.col-lg-3(for="configFileDisplay") Upload New Configuration File:
					.col-lg-5
						config-upload(id="adminUpload")
					.col-lg-1
						button.btn.btn-primary.form-control(type='submit' ng-submit='upload') Upload
			h4 Details go here
			p Something here.
